<Page
        navigatingTo="onNavigatingTo"
        xmlns="http://schemas.nativescript.org/tns.xsd"
        xmlns:pr="nativescript-pull-reveal"
>

    <ActionBar>
        <Label text="Pull Reveal Experiments"></Label>
    </ActionBar>

    <StackLayout>
        <Button text="Nested layout 1" tap="onNested1"/>
        <Button text="Nested layout 2" tap="onNested2"/>
        <Button text="Nested layout 3" tap="onNested3"/>
        <Button text="Full layout 1" tap="onFull1"/>
        <Button text="Full layout 2" tap="onFull2"/>
        <Button text="Full layout 3" tap="onFull3"/>
    </StackLayout>
</Page>
